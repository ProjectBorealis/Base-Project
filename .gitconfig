[remote "origin"]
	tagOpt = --no-tags
	prune = true
[core]
	untrackedCache = true
	whitespace = trailing-space,space-before-tab,indent-with-non-tab,tabwidth=4
	splitIndex = true
	commitGraph = true
	autocrlf = true
	fsmonitor = git-hooks/query-watchman
	hooksPath = git-hooks/
	fscache = true
	checkStat = minimal
[protocol]
	version = 2
[gc]
	auto = 100
	writeCommitGraph = true
[rerere]
	enabled = true
	autoupdate = true
[filter "lfs"]
	clean = git-lfs clean %f
	smudge = git-lfs smudge %f
[lfs]
	batch = 1
	setlockablereadonly = 1
	concurrenttransfers = 15
	tustransfers = 1
[index]
	threads = true
